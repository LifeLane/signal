{
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/supabase/:path*",
      "destination": "https://<YOUR_SUPABASE_URL>/:path*"
    }
  ],
  "functions": {
    "api/supabase-migrations": {
      "runtime": "@vercel/node@16.x",
      "memory": 3008
    }
  }
}
